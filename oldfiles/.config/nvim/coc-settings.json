{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "Markdown",
    "json",
    "sql",
    "elixir",
    "dart",
    "go"
  ],
  "coc.preferences.extensionUpdateCheck": "daily",
  "prettier.semi": false,
  "erlang_ls.erlang_ls_path": "erlang_ls",
  "erlang_ls.trace.server": "off",
  "sql.database": "postgresql",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
